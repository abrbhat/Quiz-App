<h1>Listing quiz_view_logs</h1>

<table>
  <thead>
    <tr>
      <th>Quiz</th>
      <th>Viewer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quiz_view_logs.each do |quiz_view_log| %>
      <tr>
        <td><%= quiz_view_log.quiz_id %></td>
        <td><%= quiz_view_log.viewer_id %></td>
        <td><%= link_to 'Show', quiz_view_log %></td>
        <td><%= link_to 'Edit', edit_quiz_view_log_path(quiz_view_log) %></td>
        <td><%= link_to 'Destroy', quiz_view_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiz view log', new_quiz_view_log_path %>
